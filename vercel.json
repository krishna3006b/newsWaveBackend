{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ipfs/upload",
      "dest": "/api/ipfs/upload.js"
    },
    {
      "src": "/api/ipfs/fetch/(.*)",
      "dest": "/api/ipfs/fetch.js"
    },
    {
      "src": "/api/ipfs/gateway/(.*)",
      "dest": "/api/ipfs/gateway.js"
    }
  ],
  "env": {
    "THIRDWEB_SECRET_KEY": "@thirdweb_secret_key"
  }
}
